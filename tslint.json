{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "variable-name": [
            false,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "no-console": false,
        "no-unused-variable": true,
        "forin": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
